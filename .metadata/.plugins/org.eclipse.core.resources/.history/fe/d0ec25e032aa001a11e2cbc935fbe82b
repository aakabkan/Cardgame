package com.cardgame.cardgame;

public enum CardType {
	EMPTY,	
	INCREASE_HAND_LIMIT_FOR_OPPONENT,
	GIVE_OPPONENT_SCORE,
	REMOVE_CURSE_FROM_OPPONENT,
	DECREASE_HAND_LIMIT,
	TAKE_MINUS_POINTS,
	PLAY_TOPMOST_CARD,
	TAKE_MINUS_POINTS_AND_DISCARD_HAND,
	SWITCH_SCORE_WITH_OPPONENT,
	INCREASE_PLAYED_CARDS_AND_SCORE,
}
